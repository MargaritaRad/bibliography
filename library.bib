@article{Trautmann2013a,
abstract = {Financial, managerial, and medical decisions often involve alternatives whose possible outcomes have uncertain probabilities. In contrast to alternatives whose probabilities are known, these uncertain alternatives offer the benefits of learning. In repeat-choice situations, such learning brings value. If probabilities appear favorable (unfavorable), a choice can be repeated (avoided). In a series of experiments involving bets on the colors of poker chips drawn from bags, decision makers often prove to be blind to the learning opportunities offered by uncertain probabilities. They forgo significant expected payoffs when they shun uncertain alternatives in favor of known ones. Worse, when information is revealed, many make choices contrary to learning. Priming with optimal strategies offers little improvement. Such decision makers violate identified requirements for making rational decisions. {\textcopyright} 2013 Elsevier Inc.},
author = {Trautmann, Stefan T. and Zeckhauser, Richard J.},
doi = {10.1016/j.geb.2013.01.001},
file = {:home/peisenha/ownCloud/office/literature/Trautmann&Zeckhauser.2013.pdf:pdf},
issn = {08998256},
journal = {Games and Economic Behavior},
keywords = {Gambler's fallacy,Information,Learning,Risk,Uncertainty},
number = {1},
pages = {44--55},
publisher = {Elsevier Inc.},
title = {{Shunning uncertainty: The neglect of learning opportunities}},
url = {http://dx.doi.org/10.1016/j.geb.2013.01.001},
