@Article{Heckman1943,
  Title                    = {{the Scientific Model of Causality}},
  Author                   = {Heckman, James J},
  Journal                  = {Sociological Methodology},
  Year                     = {1943},
  Number                   = {1},
  Pages                    = {1--98},
  Volume                   = {35},

  Abstract                 = {Causality is a very intuitive notion that is difficult to make precise without lapsing into tautology. Two ingredients are central to any definition: (1) a set of possible outcomes (counterfactuals) generated by a function of a set of factors or determinants and (2) a manipulation where one (or more) of the factors or determinants is changed. An effect is realized as a change in the argument of a stable function that produces the same change in the outcome for a class of interventions that change the factors by the same amount. The outcomes are compared at different levels of the factors or generating variables. Holding all factors save one at a constant level, the change in the outcome associated with manipulation of the varied factor is called a causal effect of the manipulated factor.},
  Doi                      = {10.1111/j.0081-1750.2006.00163.x},
