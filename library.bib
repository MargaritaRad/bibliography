number = {469},
pages = {94--108},
title = {{Exact and Approximate Stepdown Methods for Multiple Hypothesis Testing}},
url = {http://www.jstor.org/stable/27590521%5Cnhttp://www.jstor.org/stable/27590521?seq=1&cid=pdf-reference#references_tab_contents%5Cnhttp://about.jstor.org/terms},
