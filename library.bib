journal = {Biodemography and social biology},
keywords = {Data Collection,Data Collection: methods,Genetic Markers,Genetic Predisposition to Disease,Genetic Privacy,Genome,Genotype,Human,Humans,Longitudinal Studies,Metagenomics,Metagenomics: methods,Social Environment,Social Sciences,Social Sciences: methods,United States},
month = {jan},
