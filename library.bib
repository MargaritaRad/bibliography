journal = {Journal of Risk and Uncertainty},
keywords = {consequentialism,d81,decades,dynamic choice,dynamic consistency,in the economic literature,in the past two,jel classification,models have been presented,noneu,nonexpected utility,observed preference patterns,several nonexpected utility,these models permit empirically},
number = {2},
