@Article{Watson1983,
  Title                    = {{Alternative algorithms for the estimation of dynamic factor, mimic and varying coefficient regression models}},
  Author                   = {Watson, Mark W. and Engle, Robert F.},
  Journal                  = {Journal of Econometrics},
  Year                     = {1983},
  Number                   = {3},
