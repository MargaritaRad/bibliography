journal = {SSRN Electronic Journal},
keywords = {Credit Constraints,Education,Health Capital,Human Capital,Rational Addiction,Unhealthy Behavior,Wealth},
title = {{A Dynamic Model of Health, Education, and Wealth with Credit Constraints and Rational Addiction}},
