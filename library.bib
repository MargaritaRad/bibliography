@article{Weedon2008,
abstract = {Adult height is a model polygenic trait, but there has been limited success in identifying the genes underlying its normal variation. To identify genetic variants influencing adult human height, we used genome-wide association data from 13,665 individuals and genotyped 39 variants in an additional 16,482 samples. We identified 20 variants associated with adult height (P < 5 x 10(-7), with 10 reaching P < 1 x 10(-10)). Combined, the 20 SNPs explain approximately 3% of height variation, with a approximately 5 cm difference between the 6.2% of people with 17 or fewer 'tall' alleles compared to the 5.5% with 27 or more 'tall' alleles. The loci we identified implicate genes in Hedgehog signaling (IHH, HHIP, PTCH1), extracellular matrix (EFEMP1, ADAMTSL3, ACAN) and cancer (CDK6, HMGA2, DLEU7) pathways, and provide new insights into human growth and developmental processes. Finally, our results provide insights into the genetic architecture of a classic quantitative trait.},
author = {Weedon, Michael N and Lango, Hana and Lindgren, Cecilia M and Wallace, Chris and Evans, David M and Mangino, Massimo and Freathy, Rachel M and Perry, John R B and Stevens, Suzanne and Hall, Alistair S and Samani, Nilesh J and Shields, Beverley and Prokopenko, Inga and Farrall, Martin and Dominiczak, Anna and Johnson, Toby and Bergmann, Sven and Beckmann, Jacques S and Vollenweider, Peter and Waterworth, Dawn M and Mooser, Vincent and Palmer, Colin N a and Morris, Andrew D and Ouwehand, Willem H and Zhao, Jing Hua and Li, Shengxu and Loos, Ruth J F and Barroso, In{\^{e}}s and Deloukas, Panagiotis and Sandhu, Manjinder S and Wheeler, Eleanor and Soranzo, Nicole and Inouye, Michael and Wareham, Nicholas J and Caulfield, Mark and Munroe, Patricia B and Hattersley, Andrew T and McCarthy, Mark I and Frayling, Timothy M},
doi = {10.1038/ng.121},
file = {::},
issn = {1546-1718},
journal = {Nature genetics},
keywords = {Adult,Aged,Body Height,Body Height: genetics,Extracellular Matrix Proteins,Extracellular Matrix Proteins: genetics,Female,Genome,Genotype,Hedgehog Proteins,Hedgehog Proteins: genetics,Human,Humans,Male,Middle Aged,Neoplasm Proteins,Neoplasm Proteins: genetics,Polymorphism,Single Nucleotide},
month = {may},
